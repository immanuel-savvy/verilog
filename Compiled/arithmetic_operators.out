:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5611f94848e0 .scope module, "arithmetic_operators" "arithmetic_operators" 2 1;
 .timescale 0 0;
    .scope S_0x5611f94848e0;
T_0 ;
    %vpi_call 2 4 "$display", "5 + 10 = %d", 32'sb00000000000000000000000000001111 {0 0 0};
    %vpi_call 2 5 "$display", "5 - 10 = %d", 32'sb11111111111111111111111111111011 {0 0 0};
    %vpi_call 2 6 "$display", "10 - 5 = %d", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call 2 7 "$display", "5 * 10 = %d", 32'sb00000000000000000000000000110010 {0 0 0};
    %vpi_call 2 8 "$display", "10 / 5 = %d", 32'sb00000000000000000000000000000010 {0 0 0};
    %vpi_call 2 9 "$display", "10 %s 3 = %d", 32'sb00000000000000000000000000000001 {0 0 0};
    %vpi_call 2 10 "$display", "+5 = %d", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call 2 11 "$display", "-5 = %d", 32'sb11111111111111111111111111111011 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_0;
